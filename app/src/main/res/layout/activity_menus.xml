<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/apk/res-auto"
    xmlns:tools2="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Menus">


    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/cartesburgers"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="71dp"
        android:layout_marginEnd="292dp"
        android:layout_marginRight="292dp"
        android:src="@drawable/suivant"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView"
        app:layout_constraintTop_toBottomOf="@+id/Supreme" />

    <Button
        android:id="@+id/Cheese"
        android:layout_width="283dp"
        android:layout_height="0dp"
        android:layout_marginBottom="412dp"
        android:layout_marginEnd="53dp"
        android:layout_marginLeft="112dp"
        android:layout_marginRight="53dp"
        android:layout_marginStart="112dp"
        android:layout_marginTop="464dp"
        android:text="cheese burger"
        android:visibility="invisible"
        app:layout_constraintBottom_toTopOf="@+id/textView"
        app:layout_constraintEnd_toEndOf="@+id/imageView12"
        app:layout_constraintEnd_toStartOf="@+id/Le180"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/Le180"
        android:layout_width="274dp"
        android:layout_height="153dp"
        android:layout_marginBottom="410dp"
        android:layout_marginEnd="74dp"
        android:layout_marginRight="74dp"
        android:layout_marginTop="464dp"
        android:text="180"
        android:visibility="invisible"
        app:layout_constraintBottom_toTopOf="@+id/imageView12"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/Cheese"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/Double"
        android:layout_width="0dp"
        android:layout_height="158dp"
        android:layout_marginBottom="228dp"
        android:layout_marginEnd="54dp"
        android:layout_marginLeft="114dp"
        android:layout_marginRight="54dp"
        android:layout_marginStart="114dp"
        android:layout_marginTop="647dp"
        android:text="Double Cheese"
        android:visibility="invisible"
        app:layout_constraintBottom_toTopOf="@+id/imageView12"
        app:layout_constraintEnd_toEndOf="@+id/Cheese"
        app:layout_constraintEnd_toStartOf="@+id/Country"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/Country"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="338dp"
        android:layout_marginEnd="69dp"
        android:layout_marginRight="69dp"
        android:layout_marginTop="647dp"
        android:text="Country"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/Double"
        app:layout_constraintTop_toBottomOf="@+id/Accueil" />

    <Button
        android:id="@+id/Supreme"
        android:layout_width="0dp"
        android:layout_height="148dp"
        android:layout_marginBottom="47dp"
        android:layout_marginEnd="50dp"
        android:layout_marginLeft="114dp"
        android:layout_marginRight="50dp"
        android:layout_marginStart="114dp"
        android:layout_marginTop="839dp"
        android:text="Supreme"
        android:visibility="invisible"
        app:layout_constraintBottom_toTopOf="@+id/imageView12"
        app:layout_constraintEnd_toStartOf="@+id/Chevre"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/Chevre"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="156dp"
        android:layout_marginEnd="70dp"
        android:layout_marginRight="70dp"
        android:text="Chevre"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/Supreme"
        app:layout_constraintTop_toBottomOf="@+id/Country" />

    <Button
        android:id="@+id/Accueil"
        android:layout_width="191dp"
        android:layout_height="166dp"
        android:layout_marginBottom="328dp"
        android:layout_marginTop="155dp"
        android:text="Accueil"
        android:visibility="invisible"
        app:layout_constraintBottom_toTopOf="@+id/Country"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="0dp"
        android:layout_height="59dp"
        android:layout_marginBottom="72dp"
        android:layout_marginEnd="56dp"
        android:layout_marginLeft="43dp"
        android:layout_marginRight="56dp"
        android:layout_marginStart="43dp"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/imageView12"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Cheese" />

</android.support.constraint.ConstraintLayout>