<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Menus">

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="16dp"
        android:layout_marginStart="16dp"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/cartesburgers" />

    <Button
        android:id="@+id/Cheese"
        android:layout_width="274dp"
        android:layout_height="149dp"
        android:layout_marginLeft="132dp"
        android:layout_marginStart="132dp"
        android:layout_marginTop="460dp"
        android:text="Cheese"
        android:visibility="invisible"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/Le180"
        android:layout_width="288dp"
        android:layout_height="155dp"
        android:layout_marginLeft="58dp"
        android:layout_marginStart="58dp"
        android:layout_marginTop="460dp"
        android:text="Button"
        android:visibility="invisible"
        app:layout_constraintStart_toEndOf="@+id/Cheese"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/Double"
        android:layout_width="284dp"
        android:layout_height="152dp"
        android:layout_marginLeft="124dp"
        android:layout_marginStart="124dp"
        android:layout_marginTop="648dp"
        android:text="Double"
        android:visibility="invisible"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/Country"
        android:layout_width="278dp"
        android:layout_height="148dp"
        android:layout_marginLeft="464dp"
        android:layout_marginStart="464dp"
        android:layout_marginTop="656dp"
        android:text="Country"
        android:visibility="invisible"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/Supreme"
        android:layout_width="285dp"
        android:layout_height="158dp"
        android:layout_marginLeft="124dp"
        android:layout_marginStart="124dp"
        android:layout_marginTop="832dp"
        android:text="Supreme"
        android:visibility="invisible"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/Chevre"
        android:layout_width="281dp"
        android:layout_height="153dp"
        android:layout_marginLeft="464dp"
        android:layout_marginStart="464dp"
        android:layout_marginTop="832dp"
        android:text="Chevre"
        android:visibility="invisible"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/Accueil"
        android:layout_width="181dp"
        android:layout_height="164dp"
        android:layout_marginLeft="328dp"
        android:layout_marginStart="328dp"
        android:layout_marginTop="152dp"
        android:text="Accueil"
        android:visibility="invisible"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/Facture"
        android:layout_width="282dp"
        android:layout_height="59dp"
        android:layout_marginLeft="124dp"
        android:layout_marginStart="124dp"
        android:layout_marginTop="1012dp"
        android:text=""
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:textColor="@color/White"/>

</android.support.constraint.ConstraintLayout>